{
  "engines": {
    "composer": "^0.20.7"
  },
  "name": "supsi-edu",
  "version": "0.0.1",
  "description": "SUPSI private blockchain network to manage modules, semesters, students and grades across every department.",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/supsi-edu.bna",
