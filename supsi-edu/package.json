    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "Gionas Bonardi &amp; Brian Pulfer",
  "email": "gionas.bonardi@student.supsi.ch &amp; brian.pulfer@student.supsi.ch",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "^0.20.7",
    "composer-cli": "^0.20.7",
    "composer-client": "^0.20.7",
    "composer-common": "^0.20.7",
    "composer-connector-embedded": "^0.20.7",
    "composer-cucumber-steps": "^0.20.7",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
}
